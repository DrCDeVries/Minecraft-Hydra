    "version": "0.2.0",
    "configurations": [

        {
            "command": "npm start",
            "env": {"DEBUG":"minecrafthydra", "localDebug":"true"},
            "name": "Debug npm start",
            "request": "launch",
            "type": "node-terminal"
        },

        {
            "command": "node config/localDebug/testminecraft.js",
            "env": {"DEBUG":"minecrafthydra"},
            "name": "Debug node test minecraft",
            "request": "launch",
            "type": "node-terminal"
        },

        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\server.js"
        }
    ]
}